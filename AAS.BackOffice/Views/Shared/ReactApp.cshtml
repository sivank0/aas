@using AAS.BackOffice.Infrastructure.Version
@model AAS.BackOffice.Infrastructure.ReactApp.ReactApp
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>Система приема заявок</title>
    <link rel="icon" href="/favicon.ico" type="image/png" sizes="32x32">
    <link rel="icon" href="/favicon.ico" type="image/png" sizes="192x192">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap"/>
</head>
<body>
<div id="@Model.ContainerId"></div>

<script>
        var systemUser = @Html.Raw(Json.Serialize(Model.SystemUser));
        var sidebarLinksTree = @Html.Raw(Json.Serialize(Model.SidebarLinksTree));
        var baseConfig = @Html.Raw(Json.Serialize(Model.BaseConfig));
        var browserType = @Html.Raw(Json.Serialize(Model.BrowserType));
    </script>

<script src="@Url.Content($"~/dist/runtime.js").WithVersion()" type="text/javascript"></script>
<script src="@Url.Content($"~/dist/vendors.js").WithVersion()" type="text/javascript"></script>
<script src="@Url.Content($"~/dist/react.js").WithVersion()" type="text/javascript"></script>
<script src="@Url.Content($"~/dist/material.js").WithVersion()" type="text/javascript"></script>
<script src="@Url.Content($"~/dist/{Model.Name}.js").WithVersion()" type="text/javascript"></script>
</body>
</html>